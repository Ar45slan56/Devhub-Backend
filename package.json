{
  "name": "devhub-backend",
  "version": "1.0.0",
  "description": "Backend API for DevHub, a community platform for developers with custom authentication and real-time features.",
  "main": "server.js",
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "start": "node server.js",
    "dev": "nodemon server.js",
    "format": "prettier --write .",
    "test": "jest --detectOpenHandles",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/usama7365/Devhub-Backend.git"
  },
  "keywords": [
    "node",
    "express",
    "mongodb",
    "mongoose",
    "authentication",
    "real-time",
    "Redis",
    "Socket"
  ],
  "author": "Usama Aamir",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usama7365/Devhub-Backend/issues"
  },
  "homepage": "https://github.com/usama7365/Devhub-Backend#readme",
  "dependencies": {
    "bcrypt": "^5.1.1",
    "bcryptjs": "^3.0.2",
    "colors": "^1.4.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.7",
    "express": "^4.21.2",
    "express-session": "^1.18.1",
    "express-validator": "^7.2.1",
    "jsonwebtoken": "^9.0.2",
    "mongoose": "^8.10.1",
    "morgan": "^1.10.0",
    "nodemailer": "^6.10.0",
    "nodemon": "^3.1.9",
    "passport": "^0.7.0",
    "passport-github2": "^0.1.12",
    "socket.io": "^4.8.1",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1"
  },
  "devDependencies": {
    "jest": "^29.7.0",
    "prettier": "3.5.1",
    "supertest": "^7.1.0"
  },
  "jest": {
    "testEnvironment": "node",
    "setupFiles": [
      "./tests/setup.js"
    ],
    "testTimeout": 30000,
    "forceExit": true
  }
}
